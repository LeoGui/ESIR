	public PageSimple(){
		List repertoire = new LinkedList<Contact>;
	}
}

protected Contact chercher(List <Contact> repertoire,String nom){
	ListIterator <Contact> it = repertoire.ListIterator;
	
	while(it.hasNext()){
	if(!repertoire.contains(nom)){return null;}
	
	
	
}
